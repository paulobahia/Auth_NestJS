generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:db/dev.db"
}

model User {
  id         String   @id @default(uuid())
  name       String
  email      String   @unique  
  password   String
  type       String   @default("0")
  imagePath  String?
  createdAt  DateTime @default(now())

  @@map("users")
}

